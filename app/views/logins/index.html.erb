<p id="notice"><%= notice %></p>

<h1>Logins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logins.each do |login| %>
      <tr>
        <td><%= login.log_id %></td>
        <td><%= login.provider_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<li>
  <%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
</li>